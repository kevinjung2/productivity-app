<%=form_for @user do |f| %>
  <%=f.label :name%>
  <%=f.text_field :name%>
  <%=f.label :password%>
  <%=f.text_field :password%>
  <%=f.label :confirm_password%>
  <%=f.text_field :confirm_password%>
  <%=f.label :birthday%>
  <%=f.date_select :birthday, include_blank: true%>
  <%=f.submit%>
<%end%>
