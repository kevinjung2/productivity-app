<% @users.each do |user| %>
  <% if current_user == user %>
    <h3><%= link_to "Me", '/profile' %></h3>
  <% else %>
    <h3><%= user.name %></h3>
  <% end %>
  <ul>
    <li>Hosting <%= user.hosted_events.count %> event(s)</li>
    <li>Attending <%= user.events.count %> event(s)</li>
  </ul>
<% end %>
