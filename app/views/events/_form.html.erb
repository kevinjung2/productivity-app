<%=form_for @event do |f| %>
  <%=f.hidden :host_id, value: @user.id%>
  <%=f.label :name%>
  <%=f.text_field :name%><br>
  <%=f.label :location%>
  <%=f.text_field :location%><br>
  <%=f.label :time%>
  <%=f.datetime_field :time, min: Time.now%><br>
  <%=f.check_box_select :users, @users, :id, :name%>
  <%=f.submit%>
<%end%>
